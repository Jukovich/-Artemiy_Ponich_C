#include <iostream>

using namespace std;

int main() {

double a;

cout << "Введите вещественное число a: ";

cin >> a;

int n = 1;

double sum = 0;

while (sum <= a) {

sum += 1.0/n;

n++;

}

for (int i = 1; i < n; i++) {

cout << i << " ";

}

return 0;

}

// Пример работы программы:

// Введите вещественное число a: 2.5

// 1 2 3 4 5 6
